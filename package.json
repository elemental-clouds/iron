{
  "author": "lstatro",
  "bugs": {
    "url": "https://github.com/lstatro/iron/issues"
  },
  "dependencies": {
    "@elemental-clouds/hydrogen": "^0.0.9",
    "@elemental-clouds/neptunium": "^0.0.2",
    "@elemental-clouds/osmium": "^0.5.2",
    "@elemental-clouds/oxygen": "^0.1.2",
    "yargs": "^17.6.0"
  },
  "description": "AWS inventory and compliance scanning CLI",
  "devDependencies": {
    "eslint": "^8.23.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "ignorePatterns": [
      "build/**/*"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "homepage": "https://github.com/lstatro/iron#readme",
  "keywords": [],
  "license": "MIT",
  "main": "src/index.js",
  "name": "@elemental-clouds/iron",
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "tabWidth": 2
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elemental-clouds/iron.git"
  },
  "scripts": {
    "format": "npx prettier --check ./",
    "lint": "eslint ."
  },
  "version": "0.0.0"
}
